<div class="panel-heading">
  <h3 class="panel-title">Users</h3>
</div>
<div class="panel-body">
  <div class="row">
    <div ng-repeat="book in books">
      <div class="col-md-6">
        <div class="col-md-6">
          <h4>{{user.name}}</h4>
          <a class="btn btn-primary" href="#/books/details/{{book._id}}">View Details</a>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
